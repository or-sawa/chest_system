<% provide(:title, @user.name) %>

<head prefix="og: http://ogp.me/ns#">
    <style id="fpbf_ac93d8111_css">@font-face { font-family: 'fpbf_ac93d8111'; font-display: swap; src: url('https://s3-ap-northeast-1.amazonaws.com/fp-bf/3d4c80e6') format('woff'); }</style>
    <link rel="stylesheet" href="https://static.spearly.com/css/spearly-richtext.min.css" type="text/css"/>
    <script src="https://static.spearly.com/js/cms.js" defer></script>
    <script>window.addEventListener('DOMContentLoaded',()=>{const t=document.querySelectorAll(':not(:defined)');for(const e of t) {e.style.visibility="hidden";}; window.spearly.config.AUTH_KEY="XZJSFXLmbFdVbJJhxH0a-7jz1viIeRO4GaKTPgLaBIs"},{once:true})</script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@spearly/jquery-plugin/dist/spearly-plugin.js" defer></script>
</head>
<body class="is-preload">
    
    <!-- Header -->
    

    <!-- Main -->
    <div id="main" style="margin-top: 100px;">
        <h1>ハイクラス求人</h1>
        <section id="one">
            <div class="topicpathBlock showTopicpathBlock clearfix" style="padding-top: 40px;">
                <kagoshima-city-list data-item-page-path='jobs_show_path'>

                  <div class="jobs" style="border-top: solid 5px #5d627b; box-shadow: 0 3px 5px rgba(0, 0, 0, 0.22); padding: 20px; margin: 20px;" data-location="${values.type}"data-category="${values.category}">
                    <a>
                      <img src="{%= kagoshima-city_image %}" height="100" width="200">
                      <h3 style="font-size: 20px;">{%= kagoshima-city_title %}</h3><br>
                      <p style="font-size: 75%; line-height: 2; font-weight: 300; margin: 0; padding: 0; border: 0; font: inherit; vertical-align: baseline; display: flex; margin-block-start: 1em; margin-block-end: 1em; margin-inline-start: 0px; margin-inline-end: 0px;">{%= kagoshima-city_company %}</p>
                      <p style="margin-bottom: 3px; font-size: 87.5%; line-height: 1.6;">{%= kagoshima-city_salary %}</p>
                      <p style="font-size: 87.5%; line-height: 1.6;">{%= kagoshima-city_area %}</p>
                    </a>
                  </div>
                </kagoshima-city-list>
            </div>
        </section>
        <ul class="actions" style="margin-top: 40px; cursor: pointer;">
            <li><a>ページ上部へ</a></li>
        </ul>
    </div>
    <!-- Footer -->

    <!-- Scripts -->
    <script>
        $(".actions").click(function() {
        $("html, body").animate({
            scrollTop: 0
        }, 500);
        return false;
        });
    </script>
</body>